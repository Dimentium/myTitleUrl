{
  "manifest_version": 2,
  "name": "TitleURL",
  "version": "0.1a1",

  "description": "Add the current URL to the windows title bar",


  "permissions": [
    "<all_urls>", "tabs"
  ],
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["titleUrlPage.js"]
    }
  ]
  

}
